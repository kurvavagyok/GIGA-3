mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 34397
externalPort = 3000

[objectStorage]
defaultBucketID = "replit-objstore-6d29450d-0880-4954-bc03-4446b1be0d7c"
